//
// Drast Programming Language Examples
//

import io

struct Person {
    string first_name
    string last_name
    int age

    void :: print() {
        // Parse Variable Call
        print(self.first_name, self.last_name, self.age)
    }
}

int :: main(int argc, string[] argv) {
    // Add More Support
    Person john_doe

    john_doe.first_name = "John"
    john_doe.last_name = "Doe"
    john_doe.age = 25

    john_doe.print()

    return 0
}